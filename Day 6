#combining two dictionaries
a={1:'Korea',2:'Italy'} 
b={3:'Australia',4:'Paris'} 
a.update(b) 
print(a)

#remove a key from dictionary 
a={1:'Korea',2:'Italy'} 
a.pop(2)
print(a)

#merging two list into dictionary 
a=[1,2]
b=['apple','ball'] 
c=dict(zip(a,b)) 
print(c)

#length of set
a={'Apple','Ball','Car'} 
print(len(a))

#intersection 
a=set([1,3.14,90,40,31,56,78,60])
b=set([59,3.14,70,80,90])
c=a.intersection(b) 
print(c)
